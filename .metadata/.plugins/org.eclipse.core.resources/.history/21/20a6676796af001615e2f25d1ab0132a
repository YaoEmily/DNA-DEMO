<Form xmlns="http://www.jiuqi.com.cn/UI/Form" version="1.0">
	<!-- 界面控件 只能为 Control Composite 名空间中元素 -->
	<Controls layout="Grid[columns:2]">
		<Label id="lbl_1" layoutData="Grid[]">label</Label>
		<Text id="txt_1" layoutData="Grid[]"></Text>
	</Controls>
	<!-- 弹出菜单（控件的右键菜单和ToolBar的下拉菜单都用到） -->
	<Menus/>
</Form>
